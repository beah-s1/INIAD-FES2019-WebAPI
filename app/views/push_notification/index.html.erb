<h2>通知一覧</h2>
<table class="table">
  <tr>
    <td scope="row">ID</td>
    <td>題名</td>
    <td>本文</td>
  </tr>
  <% @notifications.each do|notification| %>
    <tr>
      <td scope="row"><%= notification.id %></td>
      <td><%= notification.title %></td>
      <td><%= notification.message %></td>
    </tr>
  <% end %>
</table>